{
    "random_seed": 23,
    "numpy_seed": 11,
    "pytorch_seed": 555,
    "dataset_reader": {
        "type": "textcat",
        "debug": false,
        "remove_labels": false,
        "max_seq_length": 50
    },
    "validation_dataset_reader": {
        "type": "textcat",
        "debug": false,
        "remove_labels": false,
        "max_seq_length": 50
    },
    "vocabulary":{
        "min_count": {
            "full": 3
        }
    },
  "datasets_for_vocab_creation": ["train"],
  "train_data_path": "/home/ubuntu/data/ag/train.jsonl",
  "validation_data_path": "/home/ubuntu/data/ag/test.jsonl",
    "model": {
        "pretrained_file": "/home/ubuntu/vae/model_logs/test1/model.tar.gz",
        "type": "semisupervised_vae"
    },
    "iterator": {
        "type": "bucket",
        "sorting_keys": [["tokens", "num_tokens"]],
        "batch_size": 32,
        "track_epoch": true
    },
     "trainer": {
        "optimizer": {
            "type": "adam",
            "lr": 0.001
        },
        "validation_metric": "+accuracy",
        "num_serialized_models_to_keep": 2,
        "num_epochs": 100,
        "grad_norm": 10.0,
        "patience": 100,
        "cuda_device": 0,
        "learning_rate_scheduler": {
            "type": "reduce_on_plateau",
            "factor": 0.5,
            "mode": "max",
            "patience": 0
        }
    }
}
